{
	"compilerOptions": {
		"lib": [
			"ES2015",
			"ES2016",
			"ES2017",
			"ES2018",
			"ES2019",
			"ES2022",
			"ES2021"
		],
		"jsx": "react",
		"target": "ESNext",
		"module": "ESNext",
		"types": [
			"."
		],
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"esModuleInterop": false,
		"jsxFactory": "UI.createElement",
		"allowSyntheticDefaultImports": true
	}
}